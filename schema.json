      },
      "familyName": {
        "type": "string"
      },
      "firstName": {
        "type": "string"
      },
      "roles": {
        "type": "array",
        "items": [
          {
            "type": "object",
            "properties": {
              "names": {
                "type": "array",
                "items": [
                  {
                    "type": "string"
                  }
                ]
              },
              "target": {
                "type": "string"
              }
            },
            "required": [
              "names",
              "target"
            ]
          }
        ]
      }
    },
